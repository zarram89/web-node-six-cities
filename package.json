{
  "name": "six-cities",
  "version": "7.0.0",
  "description": "Проект «{{projectTitle}}» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "dev:start": "npm run build && node ./dist/main.js | pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "tsc --noEmit && node --no-warnings=--no-warnings=ExperimentalWarning --loader ts-node/esm",
    "cli": "npm run ts -- ./src/cli.ts",
    "server": "json-server --watch ./mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "pino-pretty": "^13.1.2",
    "rimraf": "5.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "type": "module",
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "got": "^14.6.5",
    "inversify": "^7.10.4",
    "json-server": "^0.17.4",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  }
}